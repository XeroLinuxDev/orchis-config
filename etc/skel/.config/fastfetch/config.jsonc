{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
	"logo": {
        "source": "~/.config/fastfetch/ninja.png",
        "type": "kitty",
        "color": {"1": "magenta"},
	"color": {"2": "blue"},
	"height": 24,
        "width": 36,
        "padding": {
            "top": 2,
           "left": 3
